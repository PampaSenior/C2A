{% for Langue in Langues %}
  {%~ if loop.first -%}
    {% set Defaut = ' show active' %}
  {%~ else -%}
    {% set Defaut = '' %}
  {%~ endif -%}
  {{~ Indentation }}<div class="tab-pane fade{{ Defaut }}" id="{{ Langue[0] }}" role="tabpanel">{{ Message|trans({},'messages',Langue[0]) }}</div>
{% endfor %}