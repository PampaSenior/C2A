{%~ if Style is defined %}
  {%~ if Style in 0..1 %}
    {%~
    include 'accueil/_contenu-fontawesome.html.twig'
    with { 'Indentation' : Indentation ~ '  ',
           'Couleur' : Couleur,
           'Aleatoire': Style,
           'Jour': Jour,
           'JourEtoile' : JourEtoile }
    ~%}
  {%- else %}
    {%~
    include 'accueil/_contenu-image.html.twig'
    with { 'Indentation' : Indentation ~ '  ',
           'Jour': Jour }
    ~%}
  {%- endif %}
{%~ else %}
  {%~
  include 'accueil/_contenu-texte.html.twig'
  with { 'Indentation' : Indentation ~ '  ',
         'Jour': Jour,
         'Resultat': Resultat }
  ~%}
{%~ endif %}